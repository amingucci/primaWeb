@model Prenotazione;
@{
    ViewData["Title"] = "Prenota";
}

<script>
    function SubmitP(){
        alert("Nome inserito:" Name "<br>Email inserita:" Email);
    }
</script>

<div class="text-center">
    <h1 class="display-4">Book</h1>

    <form class="m-2" method="post" asp-action="Prenota">
        <div class="form-group">
            <label asp-for="Nome">Your name:</label>
            <input asp-for="Nome" class="form-control">
        </div>

        <div class="form-group">
            <label asp-for="Email">Your email:</label>
            <input asp-for="Email" class="form-control">
        </div>

        <button id="bottonePrenotazione" class="btn btn-primary" onclick="SubmitP()">Prenota</button>
    </form>
</div>
